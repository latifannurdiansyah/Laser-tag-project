generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model GpsData {
  id          Int      @id @default(autoincrement())
  deviceId    String   @default("Heltec-1")
  latitude    Float
  longitude   Float
  altitude    Float?
  satellites  Int?
  createdAt   DateTime @default(now())

  @@index([deviceId])
  @@index([createdAt])
}