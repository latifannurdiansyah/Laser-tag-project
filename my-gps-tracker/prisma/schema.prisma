generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model GpsLog {
  id        Int      @id @default(autoincrement())
  deviceId  String
  latitude  Float
  longitude Float
  irStatus  String?  // Format: "HIT: 0xXXXX-0xXX" atau "-"
  createdAt DateTime @default(now())

  @@map("gps_logs")
}